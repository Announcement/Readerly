{
  "name": "readerly2",
  "version": "0.2.0",
  "description": "Read with less effort.",
  "main": "bundle.js",
  "dependencies": {
    "@knod/nouislider": "^9.2.1",
    "@knod/sbd": "^2.0.2",
    "@knod/unfluff": "^1.3.2",
    "crx": "^3.2.1",
    "franc": "^3.1.1",
    "jquery": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "butternut": "^0.4.6",
    "eslint": "^4.8.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "prettier": "^1.7.4",
    "rollup": "^0.50.0",
    "rollup-plugin-amd": "^1.3.0",
    "rollup-plugin-butternut": "^0.1.0",
    "rollup-plugin-cleanup": "^1.0.1",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-prettier": "^0.3.0",
    "source-map-loader": "^0.2.2",
    "uglify-es": "^3.1.3",
    "watchify": "^3.9.0",
    "webpack": "^3.7.1"
  },
  "scripts": {
    "test": "test",
    "rollup": "rollup -c",
    "webpack": "webpack",
    "uglify": "uglifyjs --compress --mangle --source-map --output bundle.js -- main.stage2.js",
    "beautify": "uglifyjs -b -o main.stage1.pretty.js main.stage1.js && uglifyjs -b -o main.stage2.pretty.js main.stage2.js",
    "prettier": "prettier lib/**/*.js --no-semi --single-quote --write && prettier main.*.pretty.js --no-semi --single-quote --write ",
    "pretty": "npm run-script beautify && npm run-script prettier",
    "build": "npm run-script browserify",
    "build-min": "npm run-script rollup && npm run-script webpack && npm run-script uglify",
    "babel": "babel source -d header --s",
    "babelify": "browserify script.js -t babelify --outfile bundle.js",
    "browserify": "browserify --entry original/main.js --outfile distribution/bundle.js --debug",
    "watchify": "watchify original/main.js -o distribution/bundle.js",
    "watch": "watch 'npm run build' ./lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Announcement/Readerly"
  },
  "keywords": [
    "rsvp",
    "read",
    "reader",
    "parse",
    "accessibility",
    "assistive",
    "speed"
  ],
  "author": "secret",
